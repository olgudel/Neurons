    def find_agent(self):
        for i in range(1,10):
            if self.space[i].agent == True:
                return i
        
class Space:
    def __init__(self, start = False, goal = False, forbidden = False,
                 wall = False, wall_L = False, wall_R = False,
                 wall_U = False, wall_D = False):
        self.start = start
        self.goal = goal
        self.forbidden = forbidden
        self.wall = False
        self.agent = False
        
        self.wall_L = wall_L
        self.wall_R = wall_R
        self.wall_U = wall_U
        self.wall_D = wall_D

        

        

